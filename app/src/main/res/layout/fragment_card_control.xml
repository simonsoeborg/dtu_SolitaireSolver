<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".View.Fragments.Resultat">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView

            android:id="@+id/Tableau1txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau1"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine1"
            app:layout_constraintStart_toStartOf="parent" />

        <ListView
            android:id="@+id/kortene_i_Tableau1"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau1txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine1"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="100dp"
            android:background="#E91E1E"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/Tableau2txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau2"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine2"
            app:layout_constraintStart_toStartOf="@+id/TableauLine1" />

        <ListView
            android:id="@+id/kortene_i_Tableau2"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau2txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine2"
            app:layout_constraintStart_toStartOf="@id/TableauLine1"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine2"
            android:layout_width="2dp"
            android:layout_height="270dp"
            android:layout_marginStart="200dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/Tableau3txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau3"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine3"
            app:layout_constraintStart_toStartOf="@+id/TableauLine2" />

        <ListView
            android:id="@+id/kortene_i_Tableau3"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau3txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine3"
            app:layout_constraintStart_toStartOf="@+id/TableauLine2"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine3"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="300dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/Tableau4txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau4"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine4"
            app:layout_constraintStart_toStartOf="@+id/TableauLine3" />

        <ListView
            android:id="@+id/kortene_i_Tableau4"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau4txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine4"
            app:layout_constraintStart_toStartOf="@id/TableauLine3"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine4"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="400dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/Tableau5txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau5"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine5"
            app:layout_constraintStart_toStartOf="@+id/TableauLine4" />

        <ListView
            android:id="@+id/kortene_i_Tableau5"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau5txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine5"
            app:layout_constraintStart_toStartOf="@id/TableauLine4"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine5"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="500dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/Tableau6txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau6"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine6"
            app:layout_constraintStart_toStartOf="@+id/TableauLine5" />

        <ListView
            android:id="@+id/kortene_i_Tableau6"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau6txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine6"
            app:layout_constraintStart_toEndOf="@+id/TableauLine5"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine6"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="600dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/Tableau7txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tableau7"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine7"
            app:layout_constraintStart_toStartOf="@+id/TableauLine6" />

        <ListView
            android:id="@+id/kortene_i_Tableau7"
            android:layout_width="99dp"
            android:layout_height="249dp"
            app:layout_constraintBottom_toTopOf="@+id/Tableau7txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine7"
            app:layout_constraintStart_toEndOf="@id/TableauLine6"
            app:layout_constraintTop_toBottomOf="@+id/Adskilleren" />

        <View
            android:id="@+id/TableauLine7"
            android:layout_width="2dp"
            android:layout_height="411dp"
            android:layout_marginStart="700dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <View
            android:id="@+id/Adskilleren"
            android:layout_width="700dp"
            android:layout_height="2dp"
            android:layout_marginBottom="270dp"
            android:background="#E91E1E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/Stocktxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Stock"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine1"
            app:layout_constraintStart_toStartOf="parent" />


        <ListView
            android:id="@+id/Waste3Cards"
            android:layout_width="99dp"
            android:layout_height="90dp"
            app:layout_constraintBottom_toTopOf="@+id/Wastetxt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine3"
            app:layout_constraintStart_toEndOf="@+id/TableauLine1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Wastetxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Waste"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toEndOf="@+id/TableauLine3"
            app:layout_constraintStart_toStartOf="@+id/TableauLine1" />

        <ListView
            android:id="@+id/groundPile1"
            android:layout_width="99dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/Foundation1txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine4"
            app:layout_constraintStart_toEndOf="@+id/TableauLine3"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Foundation1txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Foundation1"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine4"
            app:layout_constraintStart_toEndOf="@+id/TableauLine3" />

        <ListView
            android:id="@+id/groundPile2"
            android:layout_width="99dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/Foundation2txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine5"
            app:layout_constraintStart_toEndOf="@+id/TableauLine4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Foundation2txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Foundation2"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine5"
            app:layout_constraintStart_toEndOf="@+id/TableauLine4" />

        <ListView
            android:id="@+id/groundPile3"
            android:layout_width="99dp"
            android:layout_height="30dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@+id/Foundation3txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine6"
            app:layout_constraintStart_toEndOf="@+id/TableauLine5"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Foundation3txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Foundation3"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine6"
            app:layout_constraintStart_toEndOf="@+id/TableauLine5" />

        <ListView
            android:id="@+id/groundPile4"
            android:layout_width="99dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/Foundation4txt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine7"
            app:layout_constraintStart_toEndOf="@+id/TableauLine6"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Foundation4txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:text="@string/Foundation4"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toBottomOf="@+id/Adskilleren"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine7"
            app:layout_constraintStart_toEndOf="@+id/TableauLine6" />



        <ImageView
            android:id="@+id/cardback"
            android:layout_width="64dp"
            android:layout_height="107dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/card_back"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/Stocktxt"
            app:layout_constraintEnd_toStartOf="@+id/TableauLine1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/tag_billede_igen"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="10dp"
        android:layout_gravity="center"
        android:text="@string/take_again"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="13dp"

        app:cornerRadius="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/Continue"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:layout_marginBottom="70dp"
        android:layout_marginEnd="10dp"
        android:layout_gravity="center"
        android:text="@string/Continue"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="11sp"

        app:cornerRadius="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>